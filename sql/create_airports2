CREATE TABLE airports2 (
    origin_airport           text,
    destination_airport      text,
    origin_city              text,
    destination_city         text,
    passengers               integer,
    seats                    integer,
    flights                  integer,
    distance                 integer,
    fly_date                 text,   -- we'll convert to DATE later
    origin_population        integer,
    destination_population   integer,
    org_airport_lat          text,   -- keep as text because of 'NA'
    org_airport_long         text,
    dest_airport_lat         text,
    dest_airport_long        text
);
